package application;

import java.util.ArrayList;
import java.util.List;

public class ProgramLists {

	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		list.add("Maria");
		list.add("Olavo");
		list.add("Amanda");
		list.add("Igor");
		
		for(String item : list) {
			System.out.println(item);
		}
		list.add(2, "Marco");
		for(String item : list) {
			System.out.println(item);
		}
	}

}
